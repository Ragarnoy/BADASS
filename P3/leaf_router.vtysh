hostname __ROUTER_NAME__
no ipv6 forwarding
!

interface eth0
    ip address __ROUTER_IP__/30
    ip ospf area 0
!

interface lo
    ip address __LOOPBACK_ID__/32
    ip ospf area 0
!

router bgp 1
    neighbor 1.1.1.1 remote-as 1
    neighbor 1.1.1.1 update-source lo
    !

    address-family l2vpn evpn
        neighbor 1.1.1.1 activate
        advertise-all-vni
    exit-address-family
!

router ospf
!
